<h1><%= @doctor.name %> </h1>
<h2><%= @doctor.department %></h2>
<% @doctor.appointments.each do |appointment| %>
<p><%="#{appointment.appointment_datetime.strftime('%B %d, %Y')} at #{appointment.appointment_datetime.strftime("%H:%M")}"  %></p>
<p><%= link_to appointment.patient.name, patient_path(appointment.patient) %></p>
<% end %>
